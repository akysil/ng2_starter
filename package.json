{
  "name": "ng2_starter",
  "version": "0.1.0",
  "description": "ng2 + aot builder + karma and protractor as testers",
  "scripts": {
    "clean": "rimraf aot && rimraf coverage && rimraf src/**/*.js && rimraf src/**/*.map && rimraf src/**/*.css",
    "coverage": "browser-sync start --server coverage --files coverage --port 9876",
    "build": "rimraf dist && npm run clean && npm run sass && npm run scss-css && npm run ngc && npm run rollup && cp src/index.html dist/index.html && npm run css-scss && npm run clean",
    "dev": "webpack-dev-server --config config/webpack.js --progress --inline",
    "e2e": "protractor ./config/protractor.js || exit 0",
    "browser-sync": "browser-sync",
    "ngc": "ngc -p ngcconfig.json",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "prod": "npm run build && browser-sync start --server dist --files dist --port 8081",
    "rollup": "rollup -c config/rollup.js",
    "start": "npm run clean && npm run dev",
    "test": "karma start config/karma.js --port 9875 || exit 0",
    "sass": "node-sass src -o src --output-style compressed",
    "scss-css": "replace \".scss\" \".css\" src -r --include=\"*.ts\"",
    "css-scss": "replace \".css\" \".scss\" src -r --include=\"*.ts\""
  },
  "bin": {
    "replace": "node_modules/.bin/replace",
    "browser-sync": "node_modules/.bin/browser-sync",
    "ngc": "node_modules/.bin/ngc",
    "rollup": "node_modules/.bin/rollup"
  },
  "author": "Alex Kysil",
  "license": "MIT",
  "dependencies": {
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/compiler-cli": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/platform-server": "^4.0.0",
    "@angular/router": "^4.0.0",
    "es6-shim": "^0.35.2",
    "lodash": "^4.17.4",
    "reflect-metadata": "^0.1.9",
    "rxjs": "5.0.1",
    "zone.js": "^0.8.5"
  },
  "devDependencies": {
    "@types/jasmine": "^2.5.47",
    "@types/karma": "^0.13.35",
    "@types/lodash": "^4.14.62",
    "@types/node": "^6.0.68",
    "angular2-template-loader": "^0.4.0",
    "awesome-typescript-loader": "^3.1.2",
    "browser-sync": "^2.18.6",
    "core-js": "^2.4.1",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.5",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.15.0",
    "istanbul-instrumenter-loader": "^1.2.0",
    "jasmine-core": "^2.4.1",
    "jasmine-spec-reporter": "^2.7.0",
    "karma": "^1.6.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-remap-coverage": "^0.1.4",
    "karma-simple-reporter": "^2.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "node-sass": "^4.5.2",
    "null-loader": "^0.1.1",
    "phantomjs-prebuilt": "^2.1.7",
    "protractor": "^4.0.14",
    "raw-loader": "^0.5.1",
    "replace": "^0.3.0",
    "rimraf": "^2.5.2",
    "rollup": "^0.38.1",
    "rollup-plugin-commonjs": "^6.0.1",
    "rollup-plugin-filesize": "^1.0.1",
    "rollup-plugin-node-resolve": "^2.1.1",
    "rollup-plugin-scss": "^0.2.0",
    "rollup-plugin-uglify": "^1.0.1",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "ts-node": "^1.7.2",
    "typescript": "^2.2.1",
    "webpack": "^2.3.3",
    "webpack-dev-server": "^2.4.2",
    "webpack-merge": "^0.14.0"
  }
}
